{
  "name": "whattodo",
  "version": "1.0.0",
  "description": "Todo 중심 하루 기록 + 영어 학습 통합 앱. Day 단위로 Todo, Journal, Diary를 관리하고 학습 보상을 받으세요.",
  "keywords": [
    "todo",
    "task-manager",
    "english-learning",
    "srs",
    "spaced-repetition",
    "journal",
    "diary",
    "productivity",
    "react-native",
    "expo",
    "typescript",
    "offline-first",
    "korean"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/honghyunwoo/whatTodo.git"
  },
  "author": {
    "name": "honghyunwoo",
    "url": "https://github.com/honghyunwoo"
  },
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.3",
    "@expo/webpack-config": "^19.0.1",
