    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "~19.1.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react-test-renderer": "^19.2.3",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true
