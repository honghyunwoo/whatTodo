{
  "name": "whattodo",
  "version": "1.0.0",
  "description": "Todo 중심 하루 기록 + 영어 학습 통합 앱. Day 단위로 Todo, Journal, Diary를 관리하고 학습 보상을 받으세요.",
  "keywords": [
    "todo",
    "task-manager",
    "english-learning",
    "srs",
    "spaced-repetition",
    "journal",
    "diary",
    "productivity",
    "react-native",
    "expo",
    "typescript",
    "offline-first",
    "korean"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/honghyunwoo/whatTodo.git"
  },
  "author": {
    "name": "honghyunwoo",
    "url": "https://github.com/honghyunwoo"
  },
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@sentry/react-native": "~7.2.0",
    "expo": "^54.0.0",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-device": "^8.0.10",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "^0.32.15",
    "expo-router": "~6.0.21",
    "expo-sharing": "^14.0.8",
    "expo-speech": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/node": "^25.0.3",
    "@types/react": "~19.1.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.16",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react-test-renderer": "^19.2.3",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true
}
